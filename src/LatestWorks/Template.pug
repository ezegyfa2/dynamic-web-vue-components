section.page-section(v-bind:id='id')
    .container
        dynamic-component(
            v-bind:component_infos='title_section'
        )
    .container.mt-5
        .row
            .col-sm.col-md-6.col-lg-3(v-for='image in images')
                img(v-bind:src='image.image_url' v-bind:alt='image.image_alt')
    .container.margin-top
        dynamic-component(
            v-bind:component_infos='request_offer_section'
        )
